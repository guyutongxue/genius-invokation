{
  "name": "@gi-tcg/typings",
  "version": "0.6.0",
  "description": "Typings for Genius Invokation TCG",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./verify": {
      "types": "./dist/verify.d.ts",
      "bun": "./src/verify.ts",
      "import": "./dist/verify.js"
    }
  },
  "scripts": {
    "build:json": "bun scripts/generate_schema.ts",
