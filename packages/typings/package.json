    "build": "bun build:json && bun build:ts"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "case-anything": "^2.1.13",
    "glob": "^10.2.7",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "typescript-json-schema": "^0.57.0"
  },
  "dependencies": {
    "ajv": "^8.12.0"
  }
}
