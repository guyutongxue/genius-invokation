{
  "name": "@gi-tcg/webui-core",
  "version": "0.16.1",
  "description": "Core component of @gi-tcg/webui",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "bunx --bun vite",
    "check": "tsc --noEmit",
    "build:vite:ts": "ENABLE_DTS=1 vite build",
    "build:vite:js": "vite build",
    "build": "bun build:vite:ts",
    "build:prod": "bun build:vite:js"
  },
  "dependencies": {
    "@gi-tcg/utils": "workspace:*"
  },
  "peerDependencies": {
    "solid-js": "^1.8.15"
  },
  "devDependencies": {
    "@gi-tcg/config": "workspace:*",
    "@gi-tcg/core": "workspace:*",
    "@gi-tcg/data": "workspace:*",
    "@gi-tcg/typings": "workspace:*",
    "@gi-tcg/detail-log-viewer": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@unocss/postcss": "^0.58.3",
    "eslint": "^8.56.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-solid": "^0.13.1",
    "rollup-plugin-node-externals": "^7.0.1",
    "solid-devtools": "^0.29.2",
    "unocss": "^0.58.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-solid": "^2.8.0"
  }
}
